PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX : <http://www.lehigh.edu/~zhp2/2004/0401/univ-bench.owl#>
SELECT DISTINCT ?0 ?1 ?2 
WHERE {
	?0  rdf:type :Student .
	?0  :advisor ?1  .
	?1  rdf:type :FacultyStaff .
	?0  :takesCourse ?2  .
	?1  :teacherOf ?2  .
	?2  rdf:type :Course
}
