{
  "name": "obda-benchmark",
  "description": "Benchmarking framework for OBDA query rewriters based on ChaseBench",
  "version": "0.0.1",
  "main": "index.js",
  "author": "James Errington",
  "private": true,
  "bin": {
    "obdabenchmark": "./index.js"
  },
  "scripts": {
    "loadTest": "obdabenchmark load ..\\scenarios\\LUBM\\schema.txt ..\\scenarios\\LUBM\\data\\ .\\configs\\testconfig.json",
    "rewriteTestRapid": "obdabenchmark rewrite ..\\scenarios\\V\\Q1.rq ..\\scenarios\\V\\ontology.owl rapid .\\configs\\testconfig.json",
    "rewriteTestIqaros": "obdabenchmark rewrite ..\\scenarios\\V\\Q1.rq ..\\scenarios\\V\\ontology.owl iqaros .\\configs\\testconfig.json",
    "rewriteTestGraal": "obdabenchmark rewrite ..\\scenarios\\V\\Q1.rq ..\\scenarios\\V\\ontology.owl graal .\\configs\\testconfig.json -m rewrite",
    "testGraal": "obdabenchmark test ..\\scenarios\\LUBM\\schema.txt ..\\scenarios\\LUBM\\data\\ ..\\scenarios\\V\\Q1.rq ..\\scenarios\\V\\ontology.owl graal .\\configs\\testconfig.json -m execute"
  },
  "dependencies": {
    "commander": "2.19.0",
    "pg": "7.8.0",
    "pg-copy-streams": "1.2.0"
  }
}
