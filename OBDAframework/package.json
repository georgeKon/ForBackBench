{
  "name": "obda-benchmark",
  "description": "Benchmarking framework for OBDA query rewriters based on ChaseBench",
  "version": "0.0.1",
  "main": "index.js",
  "author": "James Errington",
  "private": true,
  "bin": {
    "obdabenchmark": "./dist/index.js"
  },
  "scripts": {
    "postinstall": "node ./app/utils/postinstall",
    "lint": "tslint --project ./ --format stylish",
    "format": "yarn run lint --fix",
    "test": "mocha -r ts-node/register ./test/**/*.test.ts --pretty",
    "dev": "rm -rf ./dist/ && tsc && node dist/app/converters/converters/tgd-owl.js",
    "build": "yarn run lint && rm -rf ./dist/ && tsc",
    "loadTest": "obdabenchmark load ../../scenarios/LUBM/schema.txt ../../scenarios/LUBM/data/",
    "loadTestLarge": "obdabenchmark load ..\\scenarios\\LUBM\\schema.txt ..\\scenarios\\LUBM\\cb-data\\ .\\configs\\testconfig.json",
    "rewriteTestRapid": "obdabenchmark rewrite ..\\scenarios\\LUBM\\Q2.rq ..\\scenarios\\LUBM\\U.owl rapid .\\configs\\testconfig.json",
    "executeTest": "obdabenchmark execute ..\\scenarios\\LUBM\\Q1.ucq ..\\scenarios\\LUBM\\schema.txt .\\configs\\testconfig.json",
    "rewriteTestIqaros": "obdabenchmark rewrite ..\\scenarios\\LUBM\\Q1.rq ..\\scenarios\\LUBM\\U.owl iqaros .\\configs\\testconfig.json",
    "rewriteTestGraal": "obdabenchmark rewrite ..\\scenarios\\LUBM\\Q2.rq ..\\scenarios\\LUBM\\ontology-dllite.owl graal .\\configs\\testconfig.json -m rewrite",
    "testGraal": "obdabenchmark test ..\\scenarios\\LUBM\\schema.txt ..\\scenarios\\LUBM\\data\\ ..\\scenarios\\LUBM\\Q1.rq ..\\scenarios\\LUBM\\ontology.owl graal .\\configs\\testconfig.json -m execute",
    "testRapid": "obdabenchmark test ..\\scenarios\\LUBM\\schema.txt ..\\scenarios\\LUBM\\cb-data\\ ..\\scenarios\\LUBM\\Q1.rq ..\\scenarios\\LUBM\\ontology.owl rapid .\\configs\\testconfig.json",
    "testIqaros": "obdabenchmark test ..\\scenarios\\LUBM\\schema.txt ..\\scenarios\\LUBM\\cb-data\\ ..\\scenarios\\LUBM\\Q1.rq ..\\scenarios\\LUBM\\ontology.owl iqaros .\\configs\\testconfig.json"
  },
  "dependencies": {
    "commander": "2.19.0",
    "pg": "7.8.0",
    "pg-copy-streams": "1.2.0",
    "sparqljs": "2.2.1",
    "xml-writer": "1.7.0",
    "xml2js": "0.4.19"
  },
  "devDependencies": {
    "@types/chai": "4.1.7",
    "@types/chai-fs": "2.0.1",
    "@types/mocha": "5.2.6",
    "@types/mock-fs": "3.6.30",
    "@types/mockdate": "2.0.0",
    "@types/node": "11.9.2",
    "@types/pg": "7.4.11",
    "@types/sparqljs": "2.1.0",
    "@types/xml2js": "0.4.3",
    "chai": "4.2.0",
    "chai-fs": "2.0.0",
    "mocha": "5.2.0",
    "mocha-sinon": "2.1.0",
    "mock-fs": "4.8.0",
    "mockdate": "2.0.2",
    "pegjs": "0.10.0",
    "sinon": "7.2.3",
    "ts-node": "8.0.2",
    "tslint": "5.12.1",
    "tslint-config-standard": "8.0.1",
    "typescript": "3.3.3"
  }
}
